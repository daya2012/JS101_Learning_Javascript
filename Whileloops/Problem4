//Print the average of even numbers from 1 to 100 (both included)
// FInd the even numbers
// Count the evn numbers
// Calculate sum of even numbers
// Find the average
let i=1;
let sum=0;
let count=0;
while(i<=100){
  if(i%2==0){
    sum=sum+i;
    count++;
  }i++;
}
console.log("Average is",sum/count);
